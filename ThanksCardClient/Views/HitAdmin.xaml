<UserControl x:Class="ThanksCardClient.Views.HitAdmin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ThanksCardClient.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid HorizontalAlignment="Center" Height="450" VerticalAlignment="Center" Width="800">
        <Grid.Background>
            <ImageBrush ImageSource="/Images/HitAdmin.png"/>
        </Grid.Background>
        <Button Content="Button" Command="{Binding MenuAdmin1Command}" HorizontalAlignment="Left" Margin="24,46,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
